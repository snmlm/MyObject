<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.bingoogolapple.refreshlayout.demo"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name="cn.bingoogolapple.refreshlayout.demo.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="cn.bingoogolapple.refreshlayout.demo.ui.activity.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="cn.bingoogolapple.refreshlayout.demo.ui.activity.StickyNavActivity" />
        <activity android:name="cn.bingoogolapple.refreshlayout.demo.ui.activity.NormalRecyclerViewActivity" />
        <activity android:name="cn.bingoogolapple.refreshlayout.demo.ui.activity.NormalListViewActivity" />
        <activity android:name="cn.bingoogolapple.refreshlayout.demo.ui.activity.SwipeRecyclerViewActivity" />
        <activity android:name="cn.bingoogolapple.refreshlayout.demo.ui.activity.SwipeListViewActivity" />
        <activity android:name="cn.bingoogolapple.refreshlayout.demo.ui.activity.ScrollViewActivity" />
        <activity android:name="cn.bingoogolapple.refreshlayout.demo.ui.activity.WebViewActivity" />
        <activity android:name="cn.bingoogolapple.refreshlayout.demo.ui.activity.ViewPagerActivity" />
    </application>

</manifest>